<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="containerHBox" alignment="CENTER_LEFT" spacing="1.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label text="Word level:">
      <HBox.margin>
         <Insets right="5.0" />
      </HBox.margin>
   </Label>
    <CheckBox fx:id="unkCheckBox" mnemonicParsing="false" text="Unknown" />
    <CheckBox fx:id="a0CheckBox" mnemonicParsing="false" text="A0" />
    <CheckBox fx:id="a1CheckBox" mnemonicParsing="false" text="A1" />
    <CheckBox fx:id="a2CheckBox" mnemonicParsing="false" text="A2" />
    <CheckBox fx:id="a2pCheckBox" mnemonicParsing="false" text="A2+" />
    <Button fx:id="selectAllButton" mnemonicParsing="false" onAction="#selectAllButtonClick" text="Select all" />
</fx:root>
